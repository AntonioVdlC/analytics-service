{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "author": {
    "name": "Antonio Villagra De La Cruz"
  },
  "homepage": "",
  "bugs": {
    "email": ""
  },
  "scripts": {
    "build": "npm run build:css && remix build && npm run build:sdk",
    "prebuild:sdk": "rm -rf ./public/sdk && mkdir ./public/sdk",
    "build:sdk": "cd ./sdk && npm run build && cd .. && cp ./sdk/dist/browser.js ./public/sdk/browser.js",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --minify",
    "dev": "npm run build:sdk && concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --watch",
    "db:migrate": "prisma migrate deploy",
    "pre-commit": "lint-staged",
    "lint": "npm run lint:app && npm run lint:prisma && npm run lint:sdk",
    "lint:app": "eslint app/**/*.{ts,tsx}",
    "lint:prisma": "eslint prisma/*.ts",
    "lint:sdk": "eslint sdk/src/*.ts",
    "test": "npm run test:unit",
    "test:unit": "vitest run --coverage",
    "postinstall": "remix setup node",
    "start": "npm run db:migrate && remix-serve build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "apexcharts": "^3.33.0",
    "form-data": "^4.0.0",
    "mailgun.js": "^4.2.1",
    "marked": "^4.0.12",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "sanitize-html": "^2.7.0",
    "stripe": "^8.199.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.1",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "c8": "^7.11.0",
    "concurrently": "^7.0.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "prisma": "^3.8.1",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.1.2",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "sideEffects": false
}
